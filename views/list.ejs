<%- include("header") -%>
    <div class="box" id="heading">
        <h1>  <%= kindOfDay %> </h1>
    </div>
    
    <div class="box">
        <%    for(var i=0; i<newItems.length; i++) { %>
        <div class="item">
            <input type="checkbox">
            <p><%= newItems[i] %></p>
        </div>
        <% } %>
           
    
        <form class="item" action="/" method="post">
            <input type="text" name="item" placeholder="add item" autocomplete="off">
            <button type="submit"> + </button>
        </form>
     
    </div>
    <%- include("footer") -%>            

